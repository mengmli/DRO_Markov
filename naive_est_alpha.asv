function q_T = naive_est_alpha(k,d,T,xi)

%estimator theta_T
q_T=zeros(k,d);
for i=1:d 
for k=1:d
        sum1=0;
        for t=1:(T)
            sum1=sum1+(xi(i,t)==k);
        end
        if sum1==0
            q_T(i,k)=30^(-2);
        else
        q_T(i,k)=sum1/(T);
        end        
end
end
end